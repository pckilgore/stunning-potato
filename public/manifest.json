{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "name": "Clouty Trading Platform",
  "short_name": "Clouty",
  "description": "At the intersection of music and finance, Clouty turns real-time music data into tradeable indexes. Whether you are a fan, artist, marketer or investor, there is a place for you at the table.",
  "categories": [
    "finance",
    "entertainment",
    "music"
  ],
  "lang": "en-US",
  "start_url": "/",
  "display": "minimal-ui",
  "theme_color": "#9642FD",
  "orientation": "portrait-primary",
  "background_color": "#1E1A20",
  "icons": [
    {
      "src": "icon/android-chrome-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "icon/android-chrome-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ]
}
